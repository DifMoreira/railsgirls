<p id="notice"><%= notice %></p>

<h1>Myfirstapps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myfirstapps.each do |myfirstapp| %>
      <tr>
        <td><%= link_to 'Show', myfirstapp %></td>
        <td><%= link_to 'Edit', edit_myfirstapp_path(myfirstapp) %></td>
        <td><%= link_to 'Destroy', myfirstapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myfirstapp', new_myfirstapp_path %>
